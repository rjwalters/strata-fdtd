    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "playwright": "^1.49.1",
    "postcss": "^8.5.6",
    "puppeteer": "^24.9.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.19.4",
