    "@monaco-editor/react": "^4.7.0",
    "gif.js": "^0.2.0",
    "h5wasm": "^0.8.8",
    "jszip": "^3.10.1",
    "monaco-editor": "^0.55.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1",
    "skulpt": "^1.2.0"
